		"autoprefixer": "^10.4.16",
		"openapi-fetch": "^0.7.6",
		"postcss": "^8.4.30",
		"svelte-local-storage-store": "^0.6.0",
		"tailwindcss": "^3.3.3",
		"unplugin-icons": "^0.17.0"
