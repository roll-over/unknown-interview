{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@8.10.2",
	"scripts": {
		"dev": "vite dev --host 0.0.0.0",
		"rscript": "node ./scripts/script.js",
		"build": "vite build",
		"preview": "vite preview",
		"analyze": "pnpm vite-bundle-visualizer",
		"check-all": "run-p typecheck lint format",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint .",
		"format-check": "prettier . -c --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss",
		"format": "pnpm format-check -w",
		"test:unit": "vitest",
		"test:comp": "playwright test",
		"logs": "docker compose logs frontend -f"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@skeletonlabs/skeleton": "^2.4.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@tanstack/svelte-query": "5.6.1",
		"autoprefixer": "^10.4.16",
		"openapi-fetch": "^0.7.10",
		"postcss": "^8.4.31",
		"svelte-local-storage-store": "^0.6.4",
		"tailwindcss": "^3.3.5",
		"unplugin-icons": "^0.17.4"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.2.0",
		"@fontsource-variable/inter": "^5.0.15",
		"@fontsource-variable/montserrat": "^5.0.16",
		"@iconify-json/cil": "^1.1.5",
		"@iconify-json/ic": "^1.1.14",
		"@iconify-json/logos": "^1.1.38",
		"@iconify-json/material-symbols": "^1.1.61",
		"@playwright/experimental-ct-svelte": "^1.39.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.3",
		"@types/gtag.js": "^0.0.15",
		"@types/node": "^20.8.10",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"@vitest/coverage-v8": "^0.34.6",
		"@vitest/ui": "^0.34.6",
		"chokidar": "^3.5.3",
		"dotenv": "^16.3.1",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.1",
		"glob": "^10.3.10",
		"npm-run-all": "^4.1.5",
		"openapi-typescript": "^6.7.0",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tslib": "^2.6.2",
		"tsx": "^3.14.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vite-bundle-visualizer": "^0.10.0",
		"vitest": "^0.34.6"
	},
	"pnpm": {
		"patchedDependencies": {
			"@skeletonlabs/skeleton@2.4.0": "patches/@skeletonlabs__skeleton@2.4.0.patch",
			"@playwright/experimental-ct-svelte@1.39.0": "patches/@playwright__experimental-ct-svelte@1.39.0.patch",
			"@types/gtag.js@0.0.15": "patches/@types__gtag.js@0.0.15.patch"
		}
	}
}
