{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"packageManager": "pnpm@8.8.0",
	"scripts": {
		"dev": "vite dev --host 0.0.0.0",
		"rscript": "node ./scripts/script.js",
		"build": "vite build",
		"preview": "vite preview",
		"analyze": "pnpm vite-bundle-visualizer",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"lint": "eslint .",
		"check-format": "prettier . -c --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss",
